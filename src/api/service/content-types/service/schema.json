{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Content": {
      "type": "customField",
      "options": {
        "preset": "default"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "Cover": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
